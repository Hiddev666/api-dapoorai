generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url = env("DATABASE_URL")
}

model Test {
    uuid String @id @default(uuid())
    name String
    @@map("test")
}

model User {
    id String @id @default(uuid())
    googleId String @unique
    name String
    email String @unique
    profile_picture String
    created_at DateTime @default(now())

    @@map("users")
}
